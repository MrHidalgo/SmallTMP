include ../mixin/_mixin

doctype html
html(lang="en")

  head
    include _head

  body(
    class!= bodyClassName
    data-file!= dataPageName
  )
    block header
      include ../components/_header

      #barba-wrapper
        .barba-container
          block content

      block footer
        include ../components/_footer

  block script
    include _script